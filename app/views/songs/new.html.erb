<h2>Add Song</h2>

<%= form_for(@song) do |f|%>
<%      if @song.errors.any?%>
        <ul>
            <% @song.errors.full_messages.each do |msg|%>
                <li><%= msg %></li>
            <%end%>
        </ul>
        <%end%>
<%=     f.label 'Song Title'%>
<%=     f.text_field :title %><br>
<%=     f.label "Song Released?"%>
<%=     f.check_box :released%><br>
<%=     f.label "Song Release Year"%>
<%=     f.number_field :release_year %><br>
<%=     f.label "Artist Name"%>
<%=     f.text_field :artist_name %><br>
<%=     f.label "Genre"%>
<%=     f.text_field :genre%><br>
<%=     f.submit %>
<%end%>